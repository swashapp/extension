{
  "name": "swash-extension",
  "version": "2.5.2",
  "private": true,
  "repository": {
    "url": "https://github.com/swashapp/extension.git"
  },
  "staticFiles": {
    "staticPath": [
      "src/core/configs",
      "src/core/modules"
    ]
  },
  "scripts": {
    "build:dev": "run-s clean && parcel build src/manifest.json src/new-tab/index.html src/dashboard/index.html src/popup/index.html --target dev && run-s copy",
    "build:prod": "run-s clean:deep && parcel build src/manifest.json src/new-tab/index.html src/dashboard/index.html src/popup/index.html --target prod --reporter @parcel/reporter-bundle-analyzer && run-s copy",
    "bundle": "run-s copy && parcel src/manifest.json src/new-tab/index.html src/dashboard/index.html src/popup/index.html --target dev",
    "pack": "run-s build:prod && web-ext build -s dist --overwrite-dest",
    "clean": "shx rm -rf dist",
    "clean:deep": "shx rm -rf dist .parcel-cache",
    "precopy": "shx mkdir -p dist dist/core",
    "copy": "shx cp -r src/core/content_scripts src/core/configs src/core/modules dist/core/ && shx cp -r src/lib src/static dist/",
    "format": "prettier --write '{src,subgraph}/**/*.{js,jsx,ts,tsx,css,json}' '*.{js,jsx,ts,tsx,css,json}'",
    "lint:code": "eslint src/**/*.ts --fix",
    "lint:extension": "web-ext lint dist",
    "run:chromium": "web-ext run -t chromium --source-dir dist",
    "run:firefox": "web-ext run -t firefox-desktop --source-dir dist",
    "start:chromium": "run-s copy && run-p -c bundle run:chromium",
    "start:firefox": "run-s copy && run-p -c bundle run:firefox",
    "subgraph:codegen": "graph codegen subgraph/subgraph.yaml --output-dir subgraph/generated",
    "subgraph:build": "graph build subgraph/subgraph.yaml --output-dir subgraph/build",
    "subgraph:deploy": "graph deploy subgraph/subgraph.yaml --node https://api.thegraph.com/deploy/ subgraph/dataunion-swash-graph --output-dir subgraph/build",
    "subgraph:create-local": "graph create subgraph/subgraph.yaml --node http://localhost:8020/ subgraph/dataunion-swash-graph",
    "subgraph:remove-local": "graph remove subgraph/subgraph.yaml --node http://localhost:8020/ subgraph/dataunion-swash-graph",
    "subgraph:deploy-local": "graph deploy subgraph/subgraph.yaml --node http://localhost:8020/ --ipfs http://localhost:5001 subgraph/dataunion-swash-graph --output-dir subgraph/build"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@ethersproject/address": "5.7.0",
    "@ethersproject/bignumber": "5.7.0",
    "@ethersproject/bytes": "5.7.0",
    "@ethersproject/contracts": "5.7.0",
    "@ethersproject/providers": "5.7.0",
    "@ethersproject/signing-key": "5.7.0",
    "@ethersproject/units": "5.7.0",
    "@ethersproject/wallet": "5.7.0",
    "@mui/material": "5.10.6",
    "@mui/styles": "5.10.6",
    "@types/react-scroll": "1.8.3",
    "interweave": "12.9.0",
    "js-sha256": "0.9.0",
    "json-ptr": "3.1.1",
    "jsonpath-plus": "6.0.1",
    "jsontokens": "3.0.0",
    "jsstore": "3.10.3",
    "mdbreact": "5.1.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-collapse": "5.1.0",
    "react-copy-to-clipboard": "5.0.4",
    "react-dom": "17.0.2",
    "react-joyride": "2.3.2",
    "react-keyed-file-browser": "1.13.1",
    "react-router-dom": "5.3.0",
    "react-scroll": "1.8.4",
    "react-share": "4.4.0",
    "react-swipeable-views": "0.14.0",
    "react-toastify": "8.1.0",
    "react-widgets": "5.5.0",
    "reactstrap": "8.10.1",
    "remove": "0.1.5",
    "streamr-client": "8.4.0",
    "webextension-polyfill": "0.8.0"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.49.0",
    "@graphprotocol/graph-ts": "0.30.0",
    "@mui/codemod": "5.10.5",
    "@parcel/config-webextension": "2.4.1",
    "@parcel/reporter-bundle-analyzer": "2.4.1",
    "@parcel/transformer-image": "2.4.1",
    "@types/react": "17.0.34",
    "@types/react-copy-to-clipboard": "5.0.2",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.2",
    "@types/react-swipeable-views": "0.13.1",
    "@types/webextension-polyfill": "0.8.1",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "npm-run-all": "4.1.5",
    "parcel": "2.4.1",
    "parcel-resolver-ignore": "2.1.3",
    "prettier": "2.4.1",
    "process": "0.11.10",
    "shx": "0.3.3",
    "typescript": "4.4.4",
    "web-ext": "6.5.0"
  },
  "targets": {
    "dev": {
      "distDir": "dist",
      "optimize": false,
      "sourceMap": {
        "inline": true,
        "inlineSources": true
      }
    },
    "prod": {
      "distDir": "dist",
      "optimize": true
    }
  },
  "parcelIgnore": [
    "static/fonts/*.*",
    "static/images/*.*"
  ]
}
