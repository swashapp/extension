{
  "name": "swash-extension",
  "version": "2.10.0",
  "type": "module",
  "repository": {
    "url": "https://github.com/swashapp/extension.git"
  },
  "staticFiles": {
    "staticPath": [
      "src/core/configs",
      "src/core/modules"
    ]
  },
  "scripts": {
    "clean": "shx rm -rf dist src/manifest.json",
    "clean:deep": "shx rm -rf dist .parcel-cache src/manifest.json",
    "precopy": "shx mkdir -p dist dist/core",
    "copy": "shx cp -r src/core/content_scripts src/core/inpage_scripts src/core/configs src/core/modules dist/core/ && shx cp -r src/lib src/static dist/",
    "parcel:dev": "parcel watch --no-hmr src/manifest.json src/new-tab/index.html src/popup/index.html src/dashboard/index.html --target dev",
    "parcel:prod": "parcel build src/manifest.json src/new-tab/index.html src/popup/index.html src/dashboard/index.html --target prod --reporter @parcel/reporter-bundle-analyzer",
    "build:firefox:manifest": "node dev/prepare.js -b firefox -m 2",
    "build:firefox:dev": "run-s clean:deep build:firefox:manifest copy parcel:dev",
    "build:firefox:prod": "run-s clean:deep build:firefox:manifest copy parcel:prod",
    "build:chrome:manifest": "node dev/prepare.js -b chrome -m 3",
    "build:chrome:dev": "run-s clean:deep build:chrome:manifest copy parcel:dev",
    "build:chrome:prod": "run-s clean:deep build:chrome:manifest copy parcel:prod",
    "build:dev": "run-s build:chrome:dev",
    "build:prod": "run-s build:chrome:prod",
    "run:firefox": "web-ext run --source-dir dist",
    "run:chrome": "web-ext run -t chromium --source-dir dist",
    "run:dev": "run-s run:chrome",
    "run:prod": "run -s run:chrome",
    "run": "run-s run:chrome",
    "pack:firefox": "node dev/release.js firefox",
    "pack:chrome": "node dev/release.js chrome",
    "release:firefox": "run-s build:firefox:prod pack:firefox",
    "release:chrome": "run-s build:chrome:prod pack:chrome",
    "release": "run-s release:*",
    "format": "prettier --write '{src,subgraph}/**/*.{js,jsx,ts,tsx,css,json}' '*.{js,jsx,ts,tsx,css,json}'",
    "lint:code": "eslint src/**/*.ts --fix",
    "lint:extension": "web-ext lint dist",
    "subgraph:codegen": "graph codegen subgraph/subgraph.yaml --output-dir subgraph/generated",
    "subgraph:build": "graph build subgraph/subgraph.yaml --output-dir subgraph/build",
    "subgraph:deploy": "graph deploy subgraph/subgraph.yaml --node https://api.thegraph.com/deploy/ subgraph/dataunion-swash-graph --output-dir subgraph/build",
    "subgraph:create-local": "graph create subgraph/subgraph.yaml --node http://localhost:8020/ subgraph/dataunion-swash-graph",
    "subgraph:remove-local": "graph remove subgraph/subgraph.yaml --node http://localhost:8020/ subgraph/dataunion-swash-graph",
    "subgraph:deploy-local": "graph deploy subgraph/subgraph.yaml --node http://localhost:8020/ --ipfs http://localhost:5001 subgraph/dataunion-swash-graph --output-dir subgraph/build"
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@ethersproject/address": "5.7.0",
    "@ethersproject/bignumber": "5.7.0",
    "@ethersproject/bytes": "5.7.0",
    "@ethersproject/contracts": "5.7.0",
    "@ethersproject/providers": "5.7.0",
    "@ethersproject/signing-key": "5.7.0",
    "@ethersproject/units": "5.7.0",
    "@ethersproject/wallet": "5.7.0",
    "@mui/material": "5.15.14",
    "@mui/styles": "5.15.14",
    "fast-sha256": "1.3.0",
    "jsonpath-plus": "6.0.1",
    "jsonpointer": "5.0.1",
    "jsontokens": "4.0.1",
    "jsstore": "4.7.3",
    "mdbreact": "5.2.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-collapse": "5.1.1",
    "react-copy-to-clipboard": "5.1.0",
    "react-dom": "18.2.0",
    "react-keyed-file-browser": "1.14.0",
    "react-router-dom": "6.22.3",
    "react-share": "5.1.0",
    "react-swipeable-views": "0.14.0",
    "react-toastify": "10.0.5",
    "react-widgets": "5.8.4",
    "reactstrap": "9.2.2",
    "remove": "0.1.5",
    "streamr-client": "8.5.5-nolit.1",
    "use-persisted-state": "0.3.3",
    "webextension-polyfill": "0.10.0"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.69.1",
    "@graphprotocol/graph-ts": "0.34.0",
    "@mui/codemod": "5.15.14",
    "@parcel/config-webextension": "2.12.0",
    "@parcel/reporter-bundle-analyzer": "2.12.0",
    "@parcel/transformer-image": "2.12.0",
    "@types/react": "18.2.74",
    "@types/react-copy-to-clipboard": "5.0.7",
    "@types/react-dom": "18.2.23",
    "@types/react-swipeable-views": "0.13.5",
    "@types/use-persisted-state": "0.3.4",
    "@types/webextension-polyfill": "0.10.7",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "arg": "5.0.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "npm-run-all": "4.1.5",
    "parcel": "2.12.0",
    "parcel-resolver-ignore": "2.2.0",
    "prettier": "3.2.5",
    "process": "0.11.10",
    "shx": "0.3.4",
    "typescript": "5.4.3",
    "web-ext": "7.11.0"
  },
  "targets": {
    "dev": {
      "distDir": "dist",
      "optimize": false,
      "sourceMap": {
        "inline": true,
        "inlineSources": true
      }
    },
    "prod": {
      "distDir": "dist",
      "optimize": true,
      "sourceMap": false
    }
  },
  "parcelIgnore": [
    "static/fonts/*.*",
    "static/images/*.*"
  ]
}
