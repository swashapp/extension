{
  "applications": {
      "gecko": {
        "id": "authsaz@gmail.com"
        }
  },
  
  "background": {
    "page": "background.html"
  },

  "browser_action": {
    "browser_style": false,
    "default_icon": {
      "19": "icons/Solid green mark.svg",
      "38": "icons/Solid green mark.svg"
    },
    "default_title": "Swash",
    "default_popup": "popup/popup.html"
  },
 
  "description": "Swash is a Streamr Community Product that collects information about users browsing activities",

  "icons": {
    "48": "icons/Solid green mark.png",
    "96": "icons/Solid green mark.png",
	"128": "icons/Solid green mark.png"
  },

  "manifest_version": 2,

  "name": "Swash",
  
  "homepage_url": "https://swashapp.io/",
  
  
  "permissions": ["webNavigation","storage","bookmarks", "tabs", "webRequest", "<all_urls>", "identity", "notifications", "management", "proxy"],
    
  
 
  "version": "1.0.0"
}