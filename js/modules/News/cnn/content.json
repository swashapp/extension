{
	"content_matches": [
		"*://*.cnn.com*"
	],
	"content_mapping": {
		"win": "desktop",
		"mac": "desktop",
		"android": "desktop",
		"cros": "desktop",
		"openbsd": "desktop",
		"ios": "desktop",
		"ipados": "desktop"
	},
	"desktop": [{
			"name": "CNNLink",
			"description": "This item collects news title and news url after click",
			"viewGroup": "PInfo",
			"title": "CNN News",
			"url_match": "*://*.cnn.com*",
			"type": "event",
			"is_enabled": true,
		"events": [{
					"selector": "a",
					"event_name": "click"
				},
				{
					"selector": "a",
					"event_name": "contextmenu"
				}
			],
			"objects": [{
				"selector": "",
				"properties": [
				{
						"property": "href",
						"name": "link",
						"type": "url"
					},{
						"property": "innerText",
						"name": "title",
						"type": "text"
					}					
				]
			}]
		},
		
		{
			"name": "cnnnewsdetail",
			"description": "This item collects category, title, author, date and time of news",
			"viewGroup": "PInfo",
			"title": "cnn News details",
			"url_match": "*://*.cnn.com/*",
			"type": "event",
			"is_enabled": true,
			"events": [{
				"selector": "window",
				"event_name": "DOMContentLoaded"
			}],
			"objects": [{
				"selector": "document",
				"properties": [{
						"selector": "h1",
						"property": "innerText",
						"name": "Title",
						"type": "text"
					},
					{
						"selector": "#header-nav-container a[data-test=section-link]",
						"property": "attributes['aria-label'].value",
						"name": "category",
						"type": "text"
					},
					{
						"selector": ".metadata__byline",
						"property": "innerText",
						"name": "Author",
						"type": "text"
					},
					{
						"selector": ".update-time",
						"isRequired": true,
						"property": "innerText",
						"name": "Time",
						"type": "text"
					}
				]

			}]
		}
	]
}