{
   "browsing_filter": {
      "urls": [
         "*://search.aol.com/aol/*"
      ]
   },
   "browsing_extraInfoSpec": [],
   "items": [
      {
         "name": "aolQuery",
         "title": "Search Query",
         "viewGroup": "AOL",
         "is_enabled": true,
         "description": "This item collects all search queries that a user enter in AOL search bar",
         "filter": {
            "urls": [
               "*://search.aol.com/aol/*"
            ]
         },
         "patterns": [
            {
               "method": "GET",
               "url_pattern": "https:\\/\\/search\\.aol\\.com\\/aol\\/([^\\/\\?\\;]*)[\\?|\\;].*",
               "pattern_type": "regex",
               "param": [
                  {
                     "type": "query",
                     "key": "q",
                     "name": "query"
                  },
                  {
                     "type": "regex",
                     "group": 1,
                     "name": "category"
                  }
               ],
               "schems": [
                  {
                     "jpath": "$.query",
                     "type": "text"
                  },
                  {
                     "jpath": "$.category",
                     "type": "text"
                  }
               ]
            }
         ]
      }
   ]
}