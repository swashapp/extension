{
  "manifest_version": 2,
  "background": {
    "scripts": ["background/app-start.ts"]
  },
  "browser_action": {
    "browser_style": false,
    "default_icon": {
      "16": "static/images/swash/active-16.png",
      "48": "static/images/swash/active-48.png"
    },
    "default_title": "Swash"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>",
    "clipboardRead",
    "bookmarks",
    "downloads",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}
