{
  "url_matches": ["*://*/*"],
  "url_excludes": [
    "moz-extension://*/*",
    "*://www.google.com/",
    "*://www.google.com/?*",
    "*://www.google.com/search?*",
    "*://docs.google.com/*",
    "*://drive.google.com/*",
    "*://mail.google.com/*",
    "*://accounts.google.com/*",
    "*://outlook.live.com/*",
    "*://login.live.com/*",
    "*://mail.yahoo.com/*",
    "*://login.yahoo.com/*",
    "*://mail.aol.com/*",
    "*://login.aol.com/*",
    "*://www.icloud.com/mail/*",
    "*://mail.yandex.com/*",
    "*://passport.yandex.com/*",
    "*://www.facebook.com/login*",
    "*://www.facebook.com/v3.2/dialog/oauth*",
    "*://twitter.com/login*",
    "*://twitter.com/account*",
    "*://www.amazon.com/ap/signin*",
    "*://*.amazon.com/oauth?*",
    "*://*.amazon.com/console/*",
    "*://swashapp.io/*",
    "*://*.swashapp.io/*"
  ],
  "os_mapping": {
    "win": "desktop",
    "mac": "desktop",
    "android": "mobile",
    "cros": "desktop",
    "openbsd": "desktop",
    "ios": "mobile",
    "ipados": "desktop"
  },
  "desktop": [
    {
      "name": "displayAd",
      "title": "Display advertisement",
      "description": "This item add an advertisement to page being visited",
      "url_match": "*://*/*",
      "type": "sticky",
      "is_enabled": true,
      "size": {
        "width": 240,
        "height": 400
      },
      "position": {
        "bottom": 0,
        "right": 0
      }
    }
  ]
}
