  "desktop": [
    {
      "name": "searchQuery",
      "description": "This item collects walmart search query",
      "url_match": "*://*.walmart.com/*",
      "title": "search query",
      "viewGroup": "UX",
      "type": "event",
      "is_enabled": true,
      "events": [
        {
          "selector": "[aria-label='Search icon']",
          "event_name": "click"
        },
        {
          "selector": ".ld.ld-Search.absolute",
          "event_name": "click"
        },
        {
          "selector": "[aria-label='Search']",
          "event_name": "keydown",
          "keyCode": 13
        },
        {
          "selector": "[type='search']",
          "event_name": "keydown",
          "keyCode": 13
        }
      ],
      "objects": [
        {
          "selector": "[data-testid='search-form']",
          "properties": [
            {
              "selector": "[type='search']",
              "property": "value",
              "name": "query",
              "type": "text"
            }
          ]
        },
        {
          "selector": "document",
          "properties": [
            {
              "property": "URL",
              "name": "link",
              "type": "url"
            }
          ]
        }
      ]
    },
    {
